{
	"env": "dev",
	"cliProfile": "default",
	"region": "us-west-2",
	"appName": "ClaudiAuthApp",
	"adminEmail": "friends@niiknow.org",
	"poolDef": {
    "Policies": {
        "PasswordPolicy": {
            "MinimumLength": 8,
            "RequireUppercase": true,
            "RequireLowercase": true,
            "RequireNumbers": true,
            "RequireSymbols": true
        }
    },
    "MfaConfiguration": "OFF",
    "AdminCreateUserConfig": {
      "AllowAdminCreateUserOnly": false,
		  "UnusedAccountValidityDays": 120
    },
    "AliasAttributes": ["email"],
    "UserPoolTags": {
      "ClaudiAuth": "App"
    },
    "Schema": [
      {
        "Name": "address2",
        "StringAttributeConstraints": {
          "MaxLength": "250"
        },
        "AttributeDataType": "String"
      },
      {
        "Name": "city",
        "StringAttributeConstraints": {
            "MaxLength": "250"
        },
        "AttributeDataType": "String"
      },
      {
        "Name": "state",
        "StringAttributeConstraints": {
            "MaxLength": "50"
        },
        "AttributeDataType": "String"
      },
      {
        "Name": "postal",
        "StringAttributeConstraints": {
            "MaxLength": "50"
        },
        "AttributeDataType": "String"
      },
      {
        "Name": "country",
        "StringAttributeConstraints": {
            "MaxLength": "50"
        },
        "AttributeDataType": "String"
      },
      {
        "Name": "is_retired",
        "AttributeDataType": "Boolean"
      },
      {
        "Name": "occupation",
        "StringAttributeConstraints": {
            "MaxLength": "250"
        },
        "AttributeDataType": "String"
      },
      {
        "Name": "employer",
        "StringAttributeConstraints": {
            "MaxLength": "250"
        },
        "AttributeDataType": "String"
      },
      {
        "Name": "email_list_optin_at",
        "AttributeDataType": "DateTime"
      },
      {
        "Name": "pay_brand",
        "StringAttributeConstraints": {
            "MaxLength": "50"
        },
        "AttributeDataType": "String"
      },
      {
        "Name": "pay_last4",
        "StringAttributeConstraints": {
            "MaxLength": "4"
        },
        "AttributeDataType": "String"
      },
      {
        "Name": "pay_xmonth",
        "NumberAttributeConstraints": {
              "MaxValue": "12",
              "MinValue": "1"
	      },
        "AttributeDataType": "String"
      },
      {
        "Name": "pay_xyear",
        "NumberAttributeConstraints": {
              "MaxValue": "9999",
              "MinValue": "2010"
	      },
        "AttributeDataType": "String"
      },
      {
        "Name": "pay_cid",
        "StringAttributeConstraints": {
            "MaxLength": "250"
        },
        "AttributeDataType": "String"
      },
      {
        "Name": "pay_type",
        "StringAttributeConstraints": {
            "MaxLength": "50"
        },
        "AttributeDataType": "String"
      },
      {
        "Name": "teams",
        "AttributeDataType": "String"
      },
      {
        "Name": "rank",
        "StringAttributeConstraints": {
            "MaxLength": "50"
        },
        "AttributeDataType": "String"
      },
      {
        "Name": "uid",
        "StringAttributeConstraints": {
            "MaxLength": "100"
        },
        "AttributeDataType": "String"
      }
    ]
	},
	"clientDef": {
    "GenerateSecret": false,
    "RefreshTokenValidity": 31,
    "ExplicitAuthFlows": [
      "ADMIN_NO_SRP_AUTH"
    ]
	}
}
